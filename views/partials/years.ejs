<% yearStat.forEach((value, key, map)=> {
  if (value.quantity > 0) {
    let bar = 0;
    for (let i = 1; i <= value.quantity; i += 1) bar += 1.3;
    %>
    <div class="chart-line year-chart">
      <a href="/?filtermode=<%= FilmMode.YEAR %>&filtervalue=<%= key
      %>#movieSect">filter</a><span class="chart-key"><%= key %></span>&nbsp;&nbsp;
      <span class='chart-bar' style='width:<%= bar %>%'><%= value.quantity %></span>
    </div>
    <%
  }
})
%>

<script>
  document.querySelector("a[href*='/?sortyear=<%= sortyearmode %>']").classList.add("sortselected");
</script>