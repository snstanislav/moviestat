{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Program",
            "program": "${workspaceFolder}/server/node_modules/jest/bin/jest.js",
            "args": [
                "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "smartStep": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node-terminal",
            "name": "Run Script: debug (server)",
            "request": "launch",
            "command": "node models/evaluation.js",
            "cwd": "${workspaceFolder}\\server"
        }
    ]
}